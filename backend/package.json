{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:seed": "ts-node prisma/seed.ts",
    "db:push": "prisma db push",
    "//--- DEVELOPMENT ---": "",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:worker\"",
    "//--- TESTING ---": "",
    "test:unit": "jest tests/unit",
    "test:integration": "concurrently --kill-others --success first \"npm:dev:worker\" \"jest --runInBand tests/integration\"",
    "test": "npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "bullmq": "^5.8.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "pino": "^9.1.0",
    "pino-http": "^10.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
